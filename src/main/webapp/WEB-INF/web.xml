	</servlet>
	<servlet>
		<servlet-name>LogoutServlet</servlet-name>
		<display-name>LogoutServlet</display-name>
		<description />
		<servlet-class>com.india.digit.shop.servlet.LogoutServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ProductOperationServlet</servlet-name>
		<display-name>ProductOperationServlet</display-name>
		<description />
		<servlet-class>com.india.digit.shop.servlet.ProductOperationServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RegiterServlet</servlet-name>
		<url-pattern>/RegiterServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/LoginServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LogoutServlet</servlet-name>
		<url-pattern>/LogoutServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ProductOperationServlet</servlet-name>
		<url-pattern>/ProductOperationServlet</url-pattern>
	</servlet-mapping>
</web-app>
